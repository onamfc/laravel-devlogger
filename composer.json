{
    "name": "xantus/devlogger",
    "description": "A custom logging package for Laravel with db",
    "type": "library",
    "authors": [
        {
            "name": "Brandon Estrella",
            "email": "brandon@sitetransition.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DevLoggerPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevLoggerPackage\\Providers\\DevLoggerServiceProvider"
            ],
            "aliases": {
                "DevLogger": "DevLoggerPackage\\Facades\\DevLogger"
            }
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    }
}
