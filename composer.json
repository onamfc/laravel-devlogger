{
    "name": "onamfc/laravel-devlogger",
    "version": "1.0.0",
    "description": "A custom logging package for Laravel with database storage and automatic error catching",
    "type": "library",
    "keywords": ["laravel", "logging", "database", "error-handling"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Estrella",
            "email": "brandonestrelladev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DevLoggerPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevLoggerPackage\\Providers\\DevLoggerServiceProvider"
            ],
            "aliases": {
                "DevLogger": "DevLoggerPackage\\Facades\\DevLogger"
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}